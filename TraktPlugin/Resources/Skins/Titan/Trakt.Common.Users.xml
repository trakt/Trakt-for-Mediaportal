<window>
  <controls>
    <control>
      <description>Avatar</description>
      <type>image</type>
      <id>1</id>
      <posX>121</posX>
      <posY>256</posY>
      <height>380</height>
      <width>380</width>
      <keepaspectratio>yes</keepaspectratio>
      <zoom>yes</zoom>
      <texture>#selectedthumb</texture>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control Style="InfoLabel">
      <description>Username</description>
      <type>label</type>
      <label>#Trakt.User.Username</label>
      <id>0</id>
      <posX>566</posX>
      <posY>252</posY>
      <font>font14</font>
      <width>490</width>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control Style="InfoLabel">
      <description>FullName label</description>
      <type>label</type>
      <label>#Trakt.Translation.FullName.Label</label>
      <id>0</id>
      <width>260</width>
      <posX>566</posX>
      <posY>308</posY>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <visible>!string.equals(#Trakt.User.FullName,)</visible>
    </control>
    <control Style="InfoValueSmall">
      <description>FullName value</description>
      <type>fadelabel</type>
